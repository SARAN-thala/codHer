<header id="header" class="feature">
    <div class="fixed">
        <nav class="top-bar">
            <section class="top-bar-section">
                <ul class="left">
                    <li><a href="http://www.thoughtworks.com" target="_blank"
                           onclick="ga('send', 'event', 'tw', 'tw')">ThoughtWorks</a>
                    </li>
                </ul>
                <ul class="right">
                    <li>
                    <% if latest[:participate] %>
                        <a href="<%= latest[:participate] %>" target="_blank" rel="nofollow"
                           onclick="ga('send', 'event', 'participate', 'participate')">
                            Participate
                        </a>
                    <% end %>
                    </li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#agenda">Agenda</a></li>
                    <% if @item == latest %>
                        <li><a href="#location">Location</a></li>
                    <% end %>
                </ul>
            </section>
        </nav>
    </div>
</header>
